{
  "name": "express-blog",
  "version": "1.0.0",
  "description": "express blog",
  "main": "index.js",
  "scripts": {
    "test": "cross-env APP_ENV=test jest ./src/tests --forceExit",
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "docker": "docker-compose up --build",
    "db-sync": "node src/migrations/sync.js",
    "db-fresh": "node src/migrations/fresh.js",
    "db-alter": "node src/migrations/alter.js",
    "db-seed": "node src/migrations/seed.js",
    "db-seed-fresh": "npm run db-fresh && npm run db-seed",
    "make": "npx --prefix ./_generator hygen ",
    "make:controller": "npx --prefix ./_generator hygen controller new --name=$npm_config_name",
    "make:model": "npx --prefix ./_generator hygen model new --name=$npm_config_name",
    "make:router": "npx --prefix ./_generator hygen router new --name=$npm_config_name --scope=$npm_config_scope",
    "make:request": "npx --prefix ./_generator hygen requests new:$npm_config_type --name=$npm_config_name",
    "make:resource": "npx --prefix ./_generator hygen model new --name=$npm_config_name && npx --prefix ./_generator hygen controller new --name=$npm_config_name && npx --prefix ./_generator hygen router new --name=$npm_config_name --scope=$npm_config_scope && npx --prefix ./_generator hygen requests new:create --name=$npm_config_name && npx --prefix ./_generator hygen requests new:update --name=$npm_config_name  && npx --prefix ./_generator hygen requests new:retrieve --name=$npm_config_name && npx --prefix ./_generator hygen requests new:paginated --name=$npm_config_name && npx --prefix ./_generator hygen requests new:delete --name=$npm_config_name"
  },
  "jest": {
    "verbose": true,
    "detectOpenHandles": true,
    "testEnvironment": "node",
    "globalSetup": "./src/tests/SetUp.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trevorjpuckett/express.js.git"
  },
  "author": "trevorjpuckett",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/trevorjpuckett/express.js/issues"
  },
  "homepage": "https://github.com/trevorjpuckett/express.js#readme",
  "dependencies": {
    "@azure/identity": "^3.0.0",
    "@azure/storage-blob": "^12.11.0",
    "@slack/logger": "^3.0.0",
    "@slack/webhook": "^6.1.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "bull": "^4.10.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "hygen": "^6.2.11",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^3.0.4",
    "module-alias": "^2.2.2",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "pluralize": "^8.0.0",
    "sequelize": "^6.23.1",
    "sqlite3": "^5.1.2",
    "validatorjs": "^3.22.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.1.2",
    "nodemon": "^2.0.19",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.3.0"
  },
  "_moduleAliases": {
    "@app": "app",
    "@routes": "routes",
    "@config": "config"
  }
}
